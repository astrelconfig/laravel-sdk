{
    "name": "lorisleiva/astrel",
    "description": "TODO",
    "keywords": [
        "laravel",
        "astrel"
    ],
    "homepage": "https://github.com/lorisleiva/skeleton",
    "license": "MIT",
    "authors": [
        {
            "name": "Loris Leiva",
            "email": "loris.leiva@gmail.com",
            "homepage": "https://lorisleiva.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "guzzlehttp/guzzle": "^6.3.1|^7.0.1",
        "illuminate/contracts": "^7.0|^8.0"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0",
        "pestphp/pest": "^1.0",
        "phpunit/phpunit": "^9.3.10"
    },
    "autoload": {
        "psr-4": {
            "SustainableHustle\\Astrel\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SustainableHustle\\Astrel\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit --colors=always",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "SustainableHustle\\Astrel\\AstrelServiceProvider"
            ],
            "aliases": {
                "Astrel": "SustainableHustle\\Astrel\\Facades\\Astrel"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
